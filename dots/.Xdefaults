! *** GENERAL ***
*.scrollTtyOutput: False
*.scrollTtyKeypress: True
*.perl-ext:
*.perl-ext-common:
*.iso14755: false
*.iso14755_52: false

! *** VISUALS ***
*.depth: 32
*.scrollBar: False
*.cursorBlink: True
*.internalBorder: 20
*.cursorUnderline: True
*.font: xft:sharetechmono:size=14

